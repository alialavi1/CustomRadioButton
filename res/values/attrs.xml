<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RangeBar">
        <attr name="thumbOuterCircleWidth" format="dimension" />
        <attr name="thumbOuterCircleColor" format="reference|color" />
        <attr name="thumbInnerCircleWidth" format="dimension" />
        <attr name="thumbInnerCircleColor" format="reference|color" />
        <attr name="thumbCircleRadius" format="dimension" />
        <attr name="thumbCircleColor" format="reference|color" />
        <attr name="baseLineColor" format="reference|color"/>
        <attr name="tickCircleRadius" format="dimension" />
        <attr name="tickCircleColor" format="reference|color" />
        <attr name="tickOuterCircleWidth" format="dimension" />
        <attr name="tickOuterCircleColor" format="reference|color" />
        <attr name="baselineWeight" format="dimension" />
        <attr name="tickTextAlign">
            <enum name="right" value="2" />
            <enum name="left" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="tickIndex" format="integer" />
        <attr name="tickCount" format="integer" />
        <attr name="tickTextSize" format="dimension" />
        <attr name="tickTextNormalColor" format="reference|color" />
        <attr name="tickTextSelectedColor" format="reference|color" />
        <attr name="paddingLeft" format="dimension"/>
        <attr name="paddingRight" format="dimension"/>
    </declare-styleable>

</resources>